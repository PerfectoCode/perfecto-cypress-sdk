    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "adm-zip": "^0.4.16",
    "chai": "^4.2.0",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
