    "tests",
    "integration-tests"
  ],
  "author": "elhaya@perfectomobile.com",
  "license": "MIT",
  "bin": {
    "perfecto-cypress": "./lib/bin.js"
  },
  "bugs": {
    "url": "https://github.com/PerfectoCode/perfecto-cypress-sdk/issues"
  },
  "homepage": "https://github.com/PerfectoCode/perfecto-cypress-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "archiver": "^5.0.0",
